import java.io.*;


public class FILEIO {
	public static void main(String args[]) throws IOException{
		FileInputStream i = null;
		FileOutputStream o = null;
		try{
		i = new FileInputStream("input.txt");
		o = new FileOutputStream("output.txt");
		
		int c;
		
		while((c = i.read()) != -1){
			o.write(c);
		}
		}finally{
			if(i!=null){
				i.close();
			}
			if(o1=null){
				o.close();
			}
		}
	}

}
